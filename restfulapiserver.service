[Unit]
Description = gunicorn daemon
After = network.target

[Service]
User = ubuntu
Group = ubuntu
WorkingDirectory = /home/ubuntu/faq_chatbot_science_3rd
EnvironmentFile = /home/ubuntu/faq_chatbot_science_3rd/environment_gunicorn.env
ExecStart = /usr/lib/python3/dist-packages/gunicorn --workers 2 --bind unix:/tmp/gunicorn/sock restfulapiserver.wsgi:application

[Install]
WantedBy = multi-user.target
